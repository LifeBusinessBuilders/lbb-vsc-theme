{
  "name": "LBB Builder Light",
  "type": "light",
  "colors": {
    "editor.background": "#F7F8FC",
    "editor.foreground": "#2B2D33",
    "editorLineNumber.foreground": "#8A8DA3",
    "editorLineNumber.activeForeground": "#2B2D33",
    "editorGutter.background": "#EFF1F7",
    "editorCursor.foreground": "#0EA5E9",
    "sideBar.background": "#EEF1F6",
    "sideBar.foreground": "#1F2937",
    "sideBarSectionHeader.background": "#E3E7F1",
    "sideBarTitle.foreground": "#111827",
    "sideBar.border": "#D8DDE8",
    "activityBar.background": "#E3E7F1",
    "activityBar.foreground": "#1F2937",
    "activityBar.inactiveForeground": "#6B7280",
    "activityBarBadge.background": "#2563EB",
    "activityBarBadge.foreground": "#FFFFFF",
    "titleBar.activeBackground": "#E3E7F1",
    "titleBar.activeForeground": "#1F2937",
    "titleBar.inactiveBackground": "#E3E7F1",
    "titleBar.inactiveForeground": "#6B7280",
    "statusBar.background": "#E3E7F1",
    "statusBar.foreground": "#1F2937",
    "statusBar.noFolderBackground": "#E3E7F1",
    "statusBar.debuggingBackground": "#D7263D",
    "statusBar.debuggingForeground": "#FFFFFF",
    "panel.background": "#F1F3F8",
    "panelTitle.activeForeground": "#111827",
    "list.hoverBackground": "#E7EBF3",
    "list.activeSelectionBackground": "#DCE6F8",
    "list.activeSelectionForeground": "#111827",
    "list.inactiveSelectionBackground": "#EEF1F6",
    "list.inactiveSelectionForeground": "#111827",
    "editorGroup.border": "#D8DDE8",
    "tab.activeBackground": "#F7F8FC",
    "tab.activeForeground": "#1F2937",
    "tab.inactiveBackground": "#EEF1F6",
    "tab.inactiveForeground": "#6B7280",
    "tab.border": "#D8DDE8",
    "tab.activeBorderTop": "#60A5FA",
    "tab.hoverBackground": "#E7EBF3",
    "tab.unfocusedHoverBackground": "#EEF1F6",
    "focusBorder": "#60A5FA",
    "editor.selectionBackground": "#DCE6F8CC",
    "editor.inactiveSelectionBackground": "#DCE6F880",
    "editor.selectionHighlightBackground": "#60A5FA26",
    "editor.lineHighlightBackground": "#1118270D",
    "editor.findMatchBackground": "#F59E0B33",
    "editor.findMatchHighlightBackground": "#F59E0B1F",
    "terminal.background": "#F7F8FC",
    "terminal.foreground": "#2B2D33",
    "terminalCursor.foreground": "#0EA5E9",
    "terminal.selectionBackground": "#DCE6F8CC",
    "terminal.ansiBlack": "#111827",
    "terminal.ansiRed": "#B91C1C",
    "terminal.ansiGreen": "#047857",
    "terminal.ansiYellow": "#B26B00",
    "terminal.ansiBlue": "#2563EB",
    "terminal.ansiMagenta": "#7C3AED",
    "terminal.ansiCyan": "#0EA5E9",
    "terminal.ansiWhite": "#4B5563",
    "terminal.ansiBrightBlack": "#6B7280",
    "terminal.ansiBrightRed": "#D7263D",
    "terminal.ansiBrightGreen": "#059669",
    "terminal.ansiBrightYellow": "#F59E0B",
    "terminal.ansiBrightBlue": "#1D4ED8",
    "terminal.ansiBrightMagenta": "#A855F7",
    "terminal.ansiBrightCyan": "#0284C7",
    "terminal.ansiBrightWhite": "#111827"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#2B2D33",
        "background": "#F7F8FC"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "meta.annotation"],
      "settings": {
        "foreground": "#6B7280",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#B26B00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#7C3AED",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Variables & Identifiers",
      "scope": ["variable", "identifier", "meta.definition.variable"],
      "settings": {
        "foreground": "#2563EB"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#1D4ED8"
      }
    },
    {
      "name": "Tags & Markup",
      "scope": ["entity.name.tag", "markup.heading", "markup.bold"],
      "settings": {
        "foreground": "#A855F7"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#2B2D33"
      }
    },
    {
      "name": "Meta & Decorators",
      "scope": [
        "meta",
        "storage.type.annotation",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E11D48"
      }
    },
    {
      "name": "Injected / Embedded",
      "scope": ["source.css.embedded", "source.js.embedded"],
      "settings": {
        "foreground": "#0EA5E9"
      }
    },
    {
      "name": "Underline for Inherited Classes",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
