{
  "name": "LBB Builder Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E2E",
    "editor.foreground": "#D9E0EE",
    "editorLineNumber.foreground": "#6E6A86",
    "editorLineNumber.activeForeground": "#FFFFFF",
    "editorGutter.background": "#1A1A28",
    "editorCursor.foreground": "#89DCEB",
    "sideBar.background": "#1A1A28",
    "sideBar.foreground": "#C3BAC6",
    "sideBarSectionHeader.background": "#1E1E2E",
    "sideBarTitle.foreground": "#E0DEF4",
    "activityBar.background": "#1A1A28",
    "activityBar.foreground": "#D9E0EE",
    "activityBar.inactiveForeground": "#7F849C",
    "activityBarBadge.background": "#96CDFB",
    "activityBarBadge.foreground": "#1A1A28",
    "titleBar.activeBackground": "#1A1A28",
    "titleBar.activeForeground": "#D9E0EE",
    "statusBar.background": "#2A2B3C",
    "statusBar.foreground": "#D9E0EE",
    "statusBar.noFolderBackground": "#2A2B3C",
    "statusBar.debuggingBackground": "#D7263D",
    "statusBar.debuggingForeground": "#F9FAFB",
    "panel.background": "#1E1E2E",
    "panelTitle.activeForeground": "#D9E0EE",
    "list.hoverBackground": "#313244",
    "list.activeSelectionBackground": "#45475A",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#2A2B3C",
    "list.inactiveSelectionForeground": "#D9E0EE",
    "tab.activeBackground": "#1E1E2E",
    "tab.activeForeground": "#D9E0EE",
    "tab.inactiveBackground": "#2A2B3C",
    "tab.inactiveForeground": "#6E6A86",
    "tab.border": "#1E1E2E",
    "tab.activeBorderTop": "#96CDFB",
    "tab.hoverBackground": "#313244",
    "tab.unfocusedHoverBackground": "#2A2B3C",
    "focusBorder": "#96CDFB",
    "editor.selectionBackground": "#45475A80",
    "editor.inactiveSelectionBackground": "#2A2B3C80",
    "editor.selectionHighlightBackground": "#31324480",
    "editor.lineHighlightBackground": "#2A2B3C66",
    "editor.findMatchBackground": "#F9E2AF66",
    "editor.findMatchHighlightBackground": "#F9E2AF33",
    "editor.wordHighlightBackground": "#89B4FA26",
    "editor.wordHighlightStrongBackground": "#CBA6F726",
    "terminal.background": "#1E1E2E",
    "terminal.foreground": "#D9E0EE",
    "terminalCursor.foreground": "#89DCEB",
    "terminal.selectionBackground": "#45475A80",
    "terminal.ansiBlack": "#1A1A28",
    "terminal.ansiRed": "#F38BA8",
    "terminal.ansiGreen": "#ABE9B3",
    "terminal.ansiYellow": "#F9E2AF",
    "terminal.ansiBlue": "#89B4FA",
    "terminal.ansiMagenta": "#CBA6F7",
    "terminal.ansiCyan": "#89DCEB",
    "terminal.ansiWhite": "#D9E0EE",
    "terminal.ansiBrightBlack": "#6E6A86",
    "terminal.ansiBrightRed": "#D7263D",
    "terminal.ansiBrightGreen": "#ABE9B3",
    "terminal.ansiBrightYellow": "#FFB86C",
    "terminal.ansiBrightBlue": "#96CDFB",
    "terminal.ansiBrightMagenta": "#F5C2E7",
    "terminal.ansiBrightCyan": "#74C7EC",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#D9E0EE",
        "background": "#1E1E2E"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "meta.annotation"],
      "settings": {
        "foreground": "#6E6A86",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#ABE9B3"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#F9E2AF"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#CBA6F7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": ["constant.numeric", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#F38BA8"
      }
    },
    {
      "name": "Boolean Constants",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#E0A000"
      }
    },
    {
      "name": "Variables & Identifiers",
      "scope": ["variable", "identifier", "meta.definition.variable"],
      "settings": {
        "foreground": "#89B4FA"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#96CDFB"
      }
    },
    {
      "name": "Tags & Markup",
      "scope": ["entity.name.tag", "markup.heading", "markup.bold"],
      "settings": {
        "foreground": "#F5C2E7"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#D9E0EE"
      }
    },
    {
      "name": "Meta & Decorators",
      "scope": [
        "meta",
        "storage.type.annotation",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#F38BA8"
      }
    },
    {
      "name": "Injected / Embedded",
      "scope": ["source.css.embedded", "source.js.embedded"],
      "settings": {
        "foreground": "#74C7EC"
      }
    },
    {
      "name": "Underline for Inherited Classes",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Object Property Keys",
      "scope": ["meta.object-literal.key.js", "meta.property-name.js"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Hexadecimal Literals",
      "scope": ["constant.numeric.hex"],
      "settings": {
        "foreground": "#60A5FA"
      }
    }
  ]
}
